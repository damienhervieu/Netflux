{
    "extends": "airbnb",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 6
    },
    "rules": {
        "eol-last": 0,
        "linebreak-style": 0,
        "react/jsx-filename-extension": 0,
        "react/prefer-stateless-function": 0,
        "jsx-a11y/anchor-is-valid": 0,
        "react/prop-types": 0
    }, 
    "globals": {
        "document": true
    }
}